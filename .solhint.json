{
  "extends": "solhint:all",
  "rules": {
    "mark-callable-contracts": [
      "off"
    ],
    "reason-string": [
      "error",
      { "maxLength" : 50 }
    ],
    "bracket-align": [
      "off"
    ],
    "function-max-lines": [
      "error", 60
    ],
    "indent": [
      "warn"
    ],
    "compiler-version": [
      "error",
      "0.6.2"
    ]
  }
}
